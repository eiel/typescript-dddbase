{
  "name": "typescript-dddbase",
  "description": "dddbase for typescript",
  "version": "0.0.1",
  "author": "@kyo_ago",
  "main": "./build/index.js",
  "typings": "./build/index.d.ts",
  "devDependencies": {
    "espower-typescript": "^5.0.1",
    "fixpack": "^2.3.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^2.5.3",
    "node-localstorage": "^1.3.0",
    "power-assert": "^1.4.1",
    "typescript": "^2.0.0"
  },
  "license": "MIT",
  "repository": {},
  "scripts": {
    "build": "tsc --project ./",
    "clean": "rm -fr node_modules",
    "fix": "fixpack",
    "pretest": "tsc --noImplicitAny --outDir ./build/ --module commonjs --target es6 --rootDir ./ --noEmitOnError --inlineSourceMap --inlineSources --lib es5,dom,es2015.promise ./typings/bundle.d.ts ./test/*.ts",
    "test": "mocha --require intelli-espower-loader build/test/**/*.js"
  },
  "dependencies": {
    "@types/mocha": "^2.2.27",
    "@types/node": "^4.0.29",
    "@types/noisejs": "0.0.27"
  }
}
